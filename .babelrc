{
	"presets": [
		"@babel/preset-react",
		["@babel/preset-env", {
			"targets": {
				"node": "current"
			}
		}]
	],
	"plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        [
            "import",
            {
                "libraryName": "antd",
                "style": "true"
            }
        ],
        ["transform-require-ignore", {
			"extensions": [".less"]
        }]
	]
}