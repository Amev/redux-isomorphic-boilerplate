  "scripts": {
    "start": "webpack-dev-server --progress -d --config webpack.dev.config.js",
    "dev": "webpack-dev-server --progress -d --config webpack.dev.config.js",
    "bundle-base": "webpack --define process.env.NODE_ENV='\"production\"' --config webpack.prod-base.config.js",
    "bundle-legacy": "webpack --define process.env.NODE_ENV='\"production\"' --config webpack.prod-legacy.config.js",
    "bundle": "npm run bundle-base && npm run bundle-legacy",
    "babel": "babel app --out-dir dist",
    "prod": "NODE_PATH=./dist NODE_ENV=production node dist/server.js",
    "build": "npm run bundle && npm run babel",
    "ncu": "ncu -u"
  },
